<div class="well col-sm-6 col-sm-offset-3" style="text-align: center;">

  <h4>Episodes:</h4>
  <ul class="index-list">
    {{#each model as |episode|}}
    <li class="episodes">
      {{#link-to 'episodes.episode' episode}}
        {{episode.name}}
      {{/link-to}}
    </li>
    {{/each}}
  </ul>

</div>
